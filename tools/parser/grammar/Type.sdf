module Type

imports
  ExpOrType


exports

  sorts Type
  context-free syntax

	ExpOrType -> Type
	"(" {Type ","}* ")" "->" Type	-> Type		{cons("FunType")}
	"[" {Type ","}* "]" "->" Type	-> Type		{cons("StaticFunType")}
	"[" {Type ","}* "]"
	"(" {Type ","}* ")" "->" Type	-> Type		{cons("BiFunType")}
	"class"				-> Type		{cons("Class")}
	"class" "<" Type		-> Type		{cons("Class")}
