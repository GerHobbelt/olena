MAKEGEN = -I ../../lib/cbir2/ -I ../../lib/cb3dr/ -I /utilisateurs/arouarou/olena/milena/ -lglut -lCoin -lz -F DLINUX test_graph

ifeq ($(MAKECMDGOALS),debug)
  MODE = debug
else
  MODE = release
endif

MAIN = test_graph
ifeq ($(shell uname -m),i686)
  BITS = 32
else
  BITS = 64
endif
VERSION = 
LDFLAGS = -ldl -lglut  -L /utilisateurs/arouarou/coin -lCoin -lz

BINDIR = .
LINK = $(MAIN)
BINARY = $(MAIN).$(MODE)$(BITS)$(VERSION)

ifeq ($(MODE),debug)
  CXX = g++ -g -pipe
  CC  = gcc -g -pipe
  LD  = g++ -g -rdynamic
else
  CXX = g++ -pipe -O3 -DNDEBUG -finline --param inline-unit-growth=200 -fomit-frame-pointer
  CC  = gcc -pipe -O3 -DNDEBUG -finline --param inline-unit-growth=200 -fomit-frame-pointer
  LD  = g++ -O3 -rdynamic
endif

ifeq ($(BITS),32)
  CXX += -m32
  CC  += -m32
  LD  += -m32
else
  CXX += -fPIC -m64
  CC  += -fPIC -m64
  LD  += -fPIC -m64
endif

CXX += -DLINUX
CC += -DLINUX

LIBDIR0 = .
HDRDIR0 = $(LIBDIR0)/headers
SRCDIR0 = $(LIBDIR0)/sources
OBJDIR0 = $(LIBDIR0)/$(MODE)$(BITS)
FILES0  = test_graph
HEADERS0 = $(FILES0:%=$(HDRDIR0)/%.h)
OBJECTS0 = $(FILES0:%=$(OBJDIR0)/%.o)

LIBDIR1 = ../../lib/cbir2
HDRDIR1 = $(LIBDIR1)/headers
SRCDIR1 = $(LIBDIR1)/sources
OBJDIR1 = $(LIBDIR1)/$(MODE)$(BITS)
FILES1  =
HEADERS1 = $(FILES1:%=$(HDRDIR1)/%.h)
OBJECTS1 = $(FILES1:%=$(OBJDIR1)/%.o)

LIBDIR2 = ../../lib/cb3dr
HDRDIR2 = $(LIBDIR2)/headers
SRCDIR2 = $(LIBDIR2)/sources
OBJDIR2 = $(LIBDIR2)/$(MODE)$(BITS)
FILES2  = Face WRL Objet3D Vertex Vector MaillageTriangulaire OFF common Matrix MailleTriangulaire \
          MyRandom calcul
HEADERS2 = $(FILES2:%=$(HDRDIR2)/%.h)
OBJECTS2 = $(FILES2:%=$(OBJDIR2)/%.o)

LIBDIR3 = /utilisateurs/arouarou/olena/milena
HDRDIR3 = $(LIBDIR3)/headers
SRCDIR3 = $(LIBDIR3)/sources
OBJDIR3 = $(LIBDIR3)/$(MODE)$(BITS)
FILES3  =
HEADERS3 = $(FILES3:%=$(HDRDIR3)/%.h)
OBJECTS3 = $(FILES3:%=$(OBJDIR3)/%.o)

LIBDIRS = $(LIBDIR0) $(LIBDIR1) $(LIBDIR2) $(LIBDIR3)
OBJDIRS = $(OBJDIR0) $(OBJDIR1) $(OBJDIR2) $(OBJDIR3)
HEADERS = $(HEADERS0) $(HEADERS1) $(HEADERS2) $(HEADERS3)
OBJECTS = $(OBJECTS0) $(OBJECTS1) $(OBJECTS2) $(OBJECTS3)

all: $(BINDIR)/$(LINK)

debug: $(BINDIR)/$(LINK)

$(BINDIR)/$(LINK) : dirs $(BINDIR)/$(BINARY)
	@echo " * Lien $(BINDIR)/$(LINK) => $(BINDIR)/$(BINARY)"
	@ln -f -s $(BINARY) $(BINDIR)/$(LINK)

$(BINDIR)/$(BINARY): $(OBJECTS)
	@echo " * Edition des liens"
	@$(LD) -o $(BINDIR)/$(BINARY) $(OBJECTS) $(LDFLAGS)
dirs:
	@mkdir -p $(OBJDIRS)

clean:
	@echo "Supprime les binaires"
	@rm -f $(LINK)
	@rm -f $(MAIN).debug32$(VERSION)
	@rm -f $(MAIN).debug64$(VERSION)
	@rm -f $(MAIN).release32$(VERSION)
	@rm -f $(MAIN).release64$(VERSION)
	@echo "Supprime les fichiers temporaires de la librairie $(LIBDIR0)"
	@rm -Rf $(LIBDIR0)/debug32
	@rm -Rf $(LIBDIR0)/debug64
	@rm -Rf $(LIBDIR0)/release32
	@rm -Rf $(LIBDIR0)/release64
	@rm -Rf $(LIBDIR0)/doc
	@rm -f $(LIBDIR0)/sources/*~
	@rm -f $(LIBDIR0)/headers/*~
	@rm -f $(LIBDIR0)/headers/Inventor/*~
	@rm -f $(LIBDIR0)/headers/Inventor/C/*~
